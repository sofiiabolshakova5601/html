<script>
    $(document).ready(() => {
        const getCookie = ((name) => {
            let cookieArr = document.cookie.split(';');
            for(let i = 0; i < cookieArr.length; i++) {
                let cookiePair = cookieArr[i].split("=");
                if(name === cookiePair[0].trim()) return decodeURIComponent(cookiePair[1]);
            }
            return null;
        });
        const showPopup = () => {
            if(!getCookie('mouseleavePopup')){
                document.cookie = 'mouseleavePopup=' + true + '; path=/';
                if($('.eduChat')){
                    $('.eduChat').addClass('hideChatBot');
                }
                if($('#partnersForms')){
                    $('#partnersForms').addClass('hideChatBot');
                }
                $('body').addClass('overflow_for_body_to_banner');
                $('.popupCashback').fadeIn('100').addClass('popupCashback--open');
            }
        };
        $('.popupCashback__close').click(() => {
            if($('.eduChat')){
                $('.eduChat').removeClass('hideChatBot');
            }
            if($('#partnersForms')){
                $('#partnersForms').removeClass('hideChatBot');
            }
            $('.popupCashback').fadeOut('100').removeClass('popupCashback--open');
            $('body').removeClass('overflow_for_body_to_banner');
        });
        // показать форму "Узнайте стоимость выполнения своей работы"
        $('.m-popup-show').click(() => {
            $('.m-popup-moved-from-old-bundle-js').show();
        });
        // скрыть форму "Узнайте стоимость выполнения своей работы"
        $('.m-popup-moved-from-old-bundle-js-close').click(() => {
            $('.m-popup-moved-from-old-bundle-js').hide();
        });
        $(document).one('mouseleave', () => showPopup());
    });
</script>

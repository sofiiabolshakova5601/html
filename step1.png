&lt;mrow&gt;&lt;mo stretchy=&quot;false&quot;&gt;(&lt;/mo&gt;&lt;mo&gt;&amp;#x2212;&lt;/mo&gt;&lt;mn&gt;11&lt;/mn&gt;&lt;msup&gt;&lt;mo stretchy=&quot;false&quot;&gt;)&lt;/mo&gt;&lt;mn&gt;5&lt;/mn&gt;&lt;/msup&gt;&lt;/mrow&gt;&lt;mn&gt;4&lt;/mn&gt;&lt;/mroot&gt;&lt;/math&gt;" role="presentation"><svg xmlns:xlink="http://www.w3.org/1999/xlink" width="20.011ex" height="3.751ex" viewBox="0 -1210.2 8615.7 1615" role="img" focusable="false" style="vertical-align: -0.94ex;" aria-hidden="true"><g stroke="currentColor" fill="currentColor" stroke-width="0" transform="matrix(1 0 0 -1 0 0)"><use href="#MJMAIN-28" x="0" y="0"></use><use href="#MJMAIN-2212" x="389" y="0"></use><g transform="translate(1168,0)"><use href="#MJMAIN-31"></use><use href="#MJMAIN-31" x="500" y="0"></use></g><g transform="translate(2169,0)"><use href="#MJMAIN-29" x="0" y="0"></use><g transform="translate(389,438)"><g transform="translate(120,0)"><rect stroke="none" width="370" height="60" x="0" y="146"></rect><use transform="scale(0.5)" href="#MJMAIN-35" x="119" y="712"></use><use transform="scale(0.5)" href="#MJMAIN-34" x="119" y="-661"></use></g></g></g><use href="#MJMAIN-3D" x="3546" y="0"></use><g transform="translate(4602,0)"><use transform="scale(0.5)" href="#MJMAIN-34" x="829" y="813"></use><use href="#MJSZ1-221A" x="0" y="21"></use><rect stroke="none" width="3012" height="60" x="1000" y="812"></rect><g transform="translate(1000,0)"><use href="#MJMAIN-28" x="0" y="0"></use><use href="#MJMAIN-2212" x="389" y="0"></use><g transform="translate(1168,0)"><use href="#MJMAIN-31"></use><use href="#MJMAIN-31" x="500" y="0"></use></g><g transform="translate(2169,0)"><use href="#MJMAIN-29" x="0" y="0"></use><use transform="scale(0.707)" href="#MJMAIN-35" x="550" y="408"></use></g></g></g></g></svg><span class="MJX_Assistive_MathML" role="presentation"><math xmlns="http://www.w3.org/1998/Math/MathML"><mo stretchy="false">(</mo><mo>−</mo><mn>11</mn><msup><mo stretchy="false">)</mo><mfrac><mn>5</mn><mn>4</mn><